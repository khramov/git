[alias]
        co = checkout
        br = branch
        ci = commit
        st = status
        unstage = reset
        bra = branch -a
        amend = commit --amend
        ampush = !git amend && git push -f
        pulldev = !git co dev && git pull
        rbdev = !git co dev && git pull && "git co $1 && git rebase dev && git push -f #"
[user]
        name = Anton Khramov
